<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bs.mapper.IntroductionMapper">
	<insert id="addIntroduce"
		parameterType="com.bs.entity.Introduction">
		insert
		introduction(userId,company_name,name,position,email,telphone,address)
		value
		(#{userId},#{companyName},#{name},#{position},#{email},#{telphone},#{address})
	</insert>
</mapper>